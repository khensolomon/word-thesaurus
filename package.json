{
  "name": "word-thesaurus",
  "version": "1.0.0",
  "description": "Word thesaurus",
  "main": "./dist/index.js",
  "module": "./index.mjs",
  "types": "./types/index.d.ts",
  "-type": "module",
  "exports--": {
    ".": {
      "import": {
        "node": "./index.mjs",
        "default": "./index.js"
      },
      "require": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "lib",
    "test",
    "dist",
    "types",
    "index.mjs",
    "src/thesaurus.dat"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": []
  },
  "scripts": {
    "build": "npm run build:node",
    "build:node": "npm run build:cjs",
    "build:web": "node webpack.build.js",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "build:esm": "tsc -p tsconfig-esm.json",
    "docs": "npm run build:web && node webpack.docs-serve.js",
    "build:docs": "npm run build:web && node webpack.docs-build.js",
    "-tst": "node ./test/local.js",
    "-test": "mocha ./test/index.js",
    "tst": "mocha",
    "test": "mocha test/mocha-*.mjs",
    "test:current": "mocha test/local.mjs",
    "test:ts": "mocha -r ts-node/register test/*.ts",
    "test:ts-current": "mocha -r ts-node/register"
  },
  "docs": {
    "type": {
      "runtype": "module",
      "help": [
        "[type] need to disabled when publishing, nor never need if nothing to import from module",
        "but required in development",
        "NOTE: publish to npm before git commit"
      ]
    }
  },
  "engines": {
    "node": ">=16.0 <19"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khensolomon/word-thesaurus.git"
  },
  "keywords": [
    "Myanmar",
    "Number",
    "Notation",
    "Burmese",
    "numeral",
    "burma"
  ],
  "bugs": {
    "url": "https://github.com/khensolomon/word-thesaurus/issues"
  },
  "author": "Khen Solomon Lethil",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.4"
  }
}